      <Button fx:id="btnYes" layoutX="252.0" layoutY="376.0" mnemonicParsing="false" onAction="#onBtnYesClicked" prefHeight="32.0" prefWidth="78.0" styleClass="kalo2-background" stylesheets="@../../../../java/values/style.css" text="Yes" textFill="WHITE">
         <font>
            <Font name="Palatino Linotype Bold" size="14.0" />
         </font>
