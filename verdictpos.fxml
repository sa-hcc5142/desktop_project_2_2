<AnchorPane prefHeight="474.0" prefWidth="696.0" styleClass="pest-background" stylesheets="@../../../../java/values/style.css" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login.VerdictController">
   <children>
      <ImageView fitHeight="343.0" fitWidth="575.0" layoutX="92.0" layoutY="-20.0" pickOnBounds="true" preserveRatio="true">
