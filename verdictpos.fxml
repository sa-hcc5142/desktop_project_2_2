      <Button fx:id="btnNo" layoutX="369.0" layoutY="376.0" mnemonicParsing="false" onAction="#onBtnNoClicked" prefHeight="32.0" prefWidth="78.0" styleClass="kalo2-background" stylesheets="@../../../../java/values/style.css" text="No" textFill="WHITE">
